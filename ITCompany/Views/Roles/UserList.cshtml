@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityUser>

<div class="container py-5">
    <a asp-area="" asp-controller="Account" asp-action="Index">
        <button type="button" class="btn border-black">
            <strong>Вернуться на панель администратора</strong>
        </button>
    </a>
    <div class="card mb-4" id="tables">
        <div class="card-header">Список пользователей</div>
        <div class="card-body">
            <table class="table table-hover">
                <tbody>
                    @foreach (var user in Model)
                    {
                        <tr>
                            <td>@user.UserName</td>
                            <td>
                                <a class="btn btn-submit bm5" asp-action="Edit" asp-route-userid="@user.Id">Права доступа</a>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>